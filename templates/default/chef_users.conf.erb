<% node['stns']['server']['users'].each do |u| -%>
<%= "[users.#{u[:name]}]" %>
<%= u.reject {|k,v| k == :name}.map {|k,v| [k, v.inspect].join(' = ') }.join("\n") %>
<% end unless node['stns']['server']['users'].first.empty? -%>

<% node['stns']['server']['groups'].each do |u| -%>
<%= "[groups.#{u[:name]}]" %>
<%= u.reject {|k,v| k == :name}.map {|k,v| [k, v.inspect].join(' = ') }.join("\n") %>
<% end unless node['stns']['server']['groups'].first.empty? -%>
